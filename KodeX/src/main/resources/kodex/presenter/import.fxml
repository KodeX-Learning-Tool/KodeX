<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<AnchorPane stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane styleClass="default-page" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox prefHeight="302.0" prefWidth="340.0" styleClass="import__container" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <Label styleClass="main-view__header" text="Importieren" />
                  <Region prefHeight="21.0" prefWidth="340.0" />
                  <Label fx:id="procedureNameLabel" text="Der Verfahrensname" />
                  <Region prefHeight="200.0" prefWidth="200.0" />
                  <HBox styleClass="import__container">
                     <children>
                        <VBox fx:id="encodeImportBox" styleClass="import__container">
                           <children>
                              <Region prefHeight="10.0" />
                              <Button fx:id="encodeImportButton" mnemonicParsing="false" onAction="#handleEncodeImport" text="Datei Importieren" />
                           </children>
                        </VBox>
                        <Region prefHeight="42.0" prefWidth="200.0" />
                        <VBox fx:id="decodeImportBox" styleClass="import__container">
                           <children>
                              <Region prefHeight="10.0" />
                              <Button fx:id="decodeImportButton" mnemonicParsing="false" onAction="#handleDecodeImport" text="Datei Importieren" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
